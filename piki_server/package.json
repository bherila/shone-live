{
  "name": "piki-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "docker-compose up -d testdb",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:auth": "jest --config ./test/jest-e2e.json ./test/auth-flow.e2e-spec.ts",
    "test:e2e:demo": "jest --config ./test/jest-e2e.json ./test/demo-coffees.e2e-spec.ts",
    "posttest:e2e": "docker-compose stop testdb && docker-compose rm -f testdb",
    "db:reset": "docker-compose stop db && docker-compose rm -f db && docker-compose up -d db"
  },
  "dependencies": {
    "@asyncapi/generator": "^1.0.2",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.6.1",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.6.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "0.0.5",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.1",
    "@nestjs/platform-socket.io": "^7.6.1",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^4.7.5",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.1",
    "@ntegral/nestjs-sentry": "^1.0.4",
    "@sentry/node": "^5.29.0",
    "@sentry/tracing": "^5.29.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "agora-access-token": "^2.0.4",
    "aws-sdk": "^2.807.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "clone-buffer": "^1.0.0",
    "crypto-js": "^4.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "express-basic-auth": "^1.2.0",
    "express-list-routes": "^0.1.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "stripe": "^8.127.0",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.2.5",
    "@nestjs/testing": "^7.6.1",
    "@types/express": "^4.17.9",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "25.2.3",
    "@types/multer": "^1.4.5",
    "@types/node": "^13.13.35",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.12",
    "@types/stripe": "^7.13.25",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "26.0.1",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
